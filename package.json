{
  "name": "@sketch7/dotnet-ngx",
  "version": "1.0.0",
  "vsBuildSolution": "./Sketch7.DotnetNgx.sln",
  "scripts": {
    "start": "npm run server",
    "server": "dotnet run -p ./Sketch7.DotnetNgx.csproj --launch-profile \"App Dev\" -c Debug",
    "serve:prod": "dotnet run -p ./Sketch7.DotnetNgx.csproj --launch-profile \"App Prod\" -c Release",
    "serve:app": "ng serve",
    "build": "ng build",
    "build:rel": "npm run build -- --prod && npm run build:ssr:prod",
    "build:ssr": "ng run DotnetNgx:server:dev",
    "build:ssr:prod": "ng run DotnetNgx:server:production",
    "serve:docker": "bash ./tools/run-docker.sh",
    "___DIVIDER___": "______Following are more utils etc..._____",
    "analyze": "npm run build:app -- --prod --stats-json && npm run analyze:only",
    "analyze:only": "npx webpack-bundle-analyzer dist/stats.json",
    "serve:publish": "dotnet run ./publish/Sketch7.DotnetNgx.dll --launch-profile \"App Prod\"",
    "ng": "ng",
    "watch:ssr": "npm run build:ssr -- --watch",
    "lint": "ng lint",
    "publish:dotnet": "dotnet publish Sketch7.DotnetNgx.sln -c Release -o ./publish"
  },
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.2",
    "@angular/common": "^9.0.0-rc.2",
    "@angular/compiler": "^9.0.0-rc.2",
    "@angular/core": "^9.0.0-rc.2",
    "@angular/forms": "^9.0.0-rc.2",
    "@angular/platform-browser": "^9.0.0-rc.2",
    "@angular/platform-browser-dynamic": "^9.0.0-rc.2",
    "@angular/platform-server": "^9.0.0-rc.2",
    "@angular/router": "^9.0.0-rc.2",
    "@nguniversal/module-map-ngfactory-loader": "^9.0.0-next.6",
    "@ngx-translate/core": "^11.0.1",
    "@ssv/ngx.command": "^1.4.1",
    "@ssv/ngx.ux": "^1.1.0",
    "@ssv/signalr-client": "^1.0.1",
    "aspnet-prerendering": "^3.0.1",
    "core-js": "^2.6.5",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.0-rc.2",
    "@angular/cli": "9.0.0-rc.2",
    "@angular/compiler-cli": "^9.0.0-rc.2",
    "@angular/language-service": "^9.0.0-rc.2",
    "@odin/build-tools": "^0.5.1",
    "@odin/tslint": "^0.3.1",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.10.5",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-jasmine": "~2.0.1",
    "karma": "^4.0.0",
    "node-sass": "^4.13.0",
    "protractor": "~5.4.0",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": "~3.7.2"
  },
  "engines": {
    "node": "10.15.1"
  }
}
